anchorFile=ApiCommonShared/Model/lib/gbrowse/${projectName}na.conf
prop=datasetName
prop=datasetDisplayName
prop=datasetDescrip
prop=projectName
prop=organismAbbrevDisplay
prop=shortAttribution
prop=intronSizeLimit
>templateTextStart<
[${datasetName}Junctions]
feature       = gsnap:intron${datasetName}
sqlName       = gsnap:unifiedintronjunction
sqlParam     = intronSizeLimit:${intronSizeLimit}&&minIntronPercent:.01&&minNonContainedRatio:.2&&minContainedRatio:.05&&minContainedScore:1&&minNonContainedScore:5&&maxIsrpmMult:100&&isrpmRatio:0.5&&externalDatabaseName:${datasetName}
glyph         = anchored_arrow 
category     = sub { GBrowse::Configuration::getOntologyCategoryFromTrackName(@_, "${organismAbbrevDisplay}: Predicted Introns") }
fgcolor       = sub { GBrowse::Display::gsnapIntronColorFromStrandAndScore(shift) }
description   = 0  
height        = sub { GBrowse::Display::gsnapIntronHeightFromPercent(shift) }
linewidth     = sub { GBrowse::Display::gsnapIntronWidthFromIsAnnotated(shift) }  
balloon hover   = sub { GBrowse::PopupAndLinks::gsnapUnifiedIntronJunctionTitle(shift) }
balloon sticky  = 1  
bump density  = 1000 
label         = 0  
key          = ${datasetDisplayName} mRNAseq Splice Site Junctions aligned to ${organismAbbrevDisplay} ${shortAttribution}
citation      = sub { GBrowse::Display::gsnapIntronCitation() }
>templateTextEnd<






















