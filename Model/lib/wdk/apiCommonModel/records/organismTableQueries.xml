<wdkModel>

  <querySet name="OrganismTables" queryType="table" isCacheable="false">

      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <!-- Organism table query  -->  
      <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

      <sqlQuery name="Metrics" includeProjects="AmoebaDB,CryptoDB,GiardiaDB,MicrosporidiaDB,PiroplasmaDB,PlasmoDB,ToxoDB,TriTrypDB,EuPathDB">
            <column name="project_id"/>
            <column name="source_id"/> 
            <column name="strain"/>
            <column name="taxon"/>
            <column name="genome_source"/>
            <column name="organism"/>
            <sql>
            <![CDATA[
SELECT source_id,
       project_id,
       strain_abbrev as strain, 
       ncbi_tax_id as taxon,
       genome_source,
       organism_name as organism
FROM apidbtuning.organismattributes
            ]]>
            </sql>
        </sqlQuery> 
   </querySet>
</wdkModel>
