<wdkModel>
  <recordClassSet name="OrganismRecordClasses" excludeProjects="TrichDB">

     <recordClass name="OrganismRecordClass" displayName="Organism"> 

         <testParamValues includeProjects="CryptoDB">
             <paramValue name="source_id">cparIowaII</paramValue>
             <paramValue name="project_id">CryptoDB</paramValue>
         </testParamValues> 
         <testParamValues includeProjects="MicrosporidiaDB">
             <paramValue name="source_id">ecunEC1</paramValue>
             <paramValue name="project_id">MicrosporidiaDB</paramValue>
         </testParamValues> 
         <testParamValues includeProjects="PlasmoDB,EuPathDB">
             <paramValue name="source_id">pfal3D7</paramValue>
             <paramValue name="project_id">PlasmoDB</paramValue>
         </testParamValues> 
         <testParamValues includeProjects="ToxoDB">
             <paramValue name="source_id">tgonME49</paramValue>
             <paramValue name="project_id">ToxoDB</paramValue>
         </testParamValues> 
         <testParamValues includeProjects="TriTrypDB">
             <paramValue name="source_id">tbruLister427</paramValue>
             <paramValue name="project_id">TriTrypDB</paramValue>
         </testParamValues> 
         <testParamValues includeProjects="AmoebaDB">
             <paramValue name="source_id">ehisHM3IMSS</paramValue>
             <paramValue name="project_id">AmoebaDB</paramValue>
         </testParamValues> 

        <!-- primary key definition -->
        <primaryKeyAttribute name="primary_key" displayName="Organism">
           <columnRef>source_id</columnRef>
           <columnRef>project_id</columnRef>
           <text>
             <![CDATA[
                <i>$$species$$</i> $$strain$$
             ]]>
           </text>
        </primaryKeyAttribute>

     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
     <!-- Reporter                                                     -->
     <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

     <reporter name="xml"  displayName="XML: choose from columns and/or tables" 
               implementation="org.gusdb.wdk.model.report.XMLReporter">
     </reporter>

     <reporter name="tabular" displayName="Tab delimited (Excel): choose from columns"
	       implementation="org.gusdb.wdk.model.report.TabularReporter"/>

     <reporter name="fullRecord" displayName="Text: choose from columns and/or tables" 
	       implementation="org.gusdb.wdk.model.report.FullRecordReporter" />

     <reporter name="json" displayName="json: choose from columns and/or tables" 
	       implementation="org.gusdb.wdk.model.report.JSONReporter"/>


         <!--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
         <!-- Attributes:   defaults are: internal="false"     (AVAILABLE as column) 
                                          inReportMaker="true" (AVAILABLE in download report)   -->
         <!--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
          <textAttribute name="overview" displayName="Overview" inReportMaker="false" internal="true">
            <text>
               <![CDATA[
                   Organism: <i>$$species$$</i> $$strain$$<br />
                   Data Source: $$data_source$$<br />
                   Version: $$database_version$$<br />
                   Megabase Pairs: $$megabps$$ <br /> <br />
               ]]>
            </text>
          </textAttribute>

          <textAttribute name="URLGenomeFasta" displayName="Fasta Download Link" internal="true">            
             <text>
               <![CDATA[
               http://$$project_id$$.org/common/downloads/Current_Release/$$name_for_filenames$$/fasta/data/$$project_id$$-%%releaseVersion%%_$$name_for_filenames$$_Genome.fasta
               ]]>
            </text>
          </textAttribute>

          <textAttribute name="URLgff" displayName="GFF Download Link" internal="true">
            <text>
               <![CDATA[
               http://$$project_id$$.org/common/downloads/Current_Release/$$name_for_filenames$$/gff/data/$$project_id$$-%%releaseVersion%%_$$name_for_filenames$$.gff 
               ]]>
            </text>
          </textAttribute>


     <attributeQueryRef ref="OrganismAttributes.organismAttrs">
        <columnAttribute name="name_for_filenames" displayName="File Name" internal="true" inReportMaker="false"/>

        <columnAttribute name="project_id" displayName="Project" />
        <columnAttribute name="source_id" internal="true"/>
<!--
        <columnAttribute name="genus" displayName="Genus" />
        <columnAttribute name="species" displayName="Species" />
-->
        <columnAttribute name="strain" displayName="Strain" />

<!-- GENOME DATA TYPES -->

				<columnAttribute name="ncbi_tax_id" internal="true" />
        <linkAttribute name="ncbi_taxon_url" 
											 help="Click to access this Taxon ID in NCBI" displayName="NCBI Taxon ID" align="center" inReportMaker="false">
          <displayText>
            <![CDATA[
              $$ncbi_tax_id$$
            ]]>
          </displayText>
          <url>
            <![CDATA[
             http://www.ncbi.nlm.nih.gov/Taxonomy/Browser/wwwtax.cgi?id=$$ncbi_tax_id$$
            ]]>
          </url>
         </linkAttribute>
 
       <columnAttribute name="data_source" displayName="Data Source" />

        <columnAttribute name="database_version" displayName="Genome Version" internal="true" />
          <linkAttribute name="database_version_url" displayName="Genome Version" 
												 help="Click to access genome details in our DataSets page" align="right" inReportMaker="false">
          <displayText>
            <![CDATA[
              $$database_version$$
            ]]>
          </displayText>
          <url>
            <![CDATA[
              getDataset.do?display=detail#Genomes and Annotation
            ]]>
          </url>
          </linkAttribute> 

        <columnAttribute name="megabps" displayName="Mega base Pairs" internal="true" />
          <linkAttribute name="megabpsurl" displayName="Mega base Pairs" 
												 help="Available Megabase pairs; click to run a search and get all genomic sequences for this genome" align="right" inReportMaker="false">
          <displayText>
            <![CDATA[
              $$megabps$$
            ]]>
          </displayText>
          <url>
            <![CDATA[
              showSummary.do?questionFullName=GenomicSequenceQuestions.SequencesByTaxon&megabps=$$megabps$$&array(organism)=$$organism_name$$
            ]]>
          </url>
          </linkAttribute> 

          <linkAttribute name="genecounturl" displayName="Total Gene Count" 
												 help="Total Gene Count; click to run a search and get all genes annotated in this genome" align="right" inReportMaker="false">
          <displayText>
            <![CDATA[
              $$genecount$$
            ]]>
          </displayText>
          <url>
            <![CDATA[
             showSummary.do?questionFullName=GeneQuestions.GenesByTaxon&genecount=$$genecount_number$$&array(organism)=$$organism_name$$ 
            ]]>
          </url>
          </linkAttribute> 

<!-- GENE METRICS -->
        <columnAttribute name="genecount" displayName="All Genes" 
												 help="Total Gene Count" align="right" />
        <columnAttribute name="genecount_number" displayName="All Genes" 
												 help="Total Gene Count" align="right" internal="true" inReportMaker="false"/>
        <columnAttribute name="codinggenecount" displayName="Prot Genes" 
												 help="Protein Coding Gene Count: it includes pseudogenes" align="right"/>
        <columnAttribute name="pseudogenecount" displayName="All Pseudo Genes" 
												 help="Pseudo Gene Count" align="right"/>
        <columnAttribute name="othergenecount" displayName="Non Prot Genes" 
												 help="Number of Genes that code for products other than proteins, for example: rRNA,tRNA,etc." align="right"/>
        <columnAttribute name="ecnumbercount" displayName="EC Num bers" 
												 help="Number of genes with assigned Enzyme Commission numbers" align="right"/>
        <columnAttribute name="gocount" displayName="GO Terms" 
												 help="Number of genes with assigned Gene Ontology terms" align="right" />
        <columnAttribute name="isolatecount" displayName="Isolates" align="right" 
												 help="Number of Genes that map to Isolates "/>
        <columnAttribute name="sagetagcount" displayName="Sage Tags" 
												 help="Number of genes that have evidence of transcript expression based on SAGE Tags studies" align="right" />
        <columnAttribute name="rnaseqcount" displayName="RNA Seq" 
												 help="Number of genes that have evidence for transcript expression based on Transcriptome sequencing (RNA Seq) experiments" align="right" />
        <columnAttribute name="chipchipgenecount" displayName="ChIP chip" 
												 help="Number of genes that have evidence of transcript expression based on the ChIP on chip studies" align="right" />
        <columnAttribute name="rtpcrcount" displayName="RT- PCR" 
												 help="Number of genes that have evidence for transcript expression based on Real Time PCR experiments" align="right"/>
        <columnAttribute name="estcount" displayName="ESTs" 
												 help="Number of genes that have evidence of transcript expression based on Expressed Sequence Tags studies" align="right"/>
        <columnAttribute name="snpcount" displayName="SNPs" 
												 help="Number of genes that have Single Nucleotide Polymorphism(s)" align="right" />
        <columnAttribute name="orthologcount" displayName="Ortho logs" 
												 help="Number of genes that have orthologous genes" align="right"/>
        <columnAttribute name="arraygenecount" displayName="Micro arrays" 
												 help="Number of genes that have evidence of transcript expression based on Microarray studies" align="right" />
        <columnAttribute name="proteomicscount" displayName="Prote omics" 
												 help="Number of genes that have evidence of protein expression based on Mass Spectrometry studies" align="right"/>
        <columnAttribute name="tfbscount" displayName="TF Bind ing Site" 
												 help="Total number of genes having evidence of expression based on transcription factor binding site identification experiments" align="right" />
        <columnAttribute name="communitycount" displayName="Comm unity Entries" 
												 help="Number of Ggenes with comments/notes from the research community " align="right"/>


        <columnAttribute name="organism_name" displayName="Organism (Internal)" internal="true" inReportMaker="false"/>
        <columnAttribute name="species" displayName="Species" internal="false" inReportMaker="true"/>
        <columnAttribute name="is_annotated_genome"  displayName="Is Annotated" internal="true"   />

        <textAttribute name="organism" displayName="Organism" 
                                       help="The biological sample used to sequence this genome"> 
             <display><![CDATA[ <i>$$species$$</i> $$strain$$ ]]></display>
             <text><![CDATA[ $$organism_name$$  ]]></text>
        </textAttribute>

		
