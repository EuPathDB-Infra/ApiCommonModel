     <table name="organism" includeProjects="CryptoDB,PlasmoDB,ToxoDB,EuPathDB,GiardiaDB"
            displayName="Organism"
            queryRef="IsolateTables.organism">
        <columnAttribute name="organism" internal="true"/>
