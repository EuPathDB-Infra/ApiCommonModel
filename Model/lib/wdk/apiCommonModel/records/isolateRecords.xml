     </table>

     <table name="product" includeProjects="CryptoDB,PlasmoDB,ToxoDB,EuPathDB,GiardiaDB"
            displayName="Product"
            queryRef="IsolateTables.product">
        <columnAttribute name="product" internal="true"/>
     </table>

     <table name="strain" includeProjects="CryptoDB,PlasmoDB,ToxoDB,EuPathDBi,GiardiaDB"
            displayName="Strain"
            queryRef="IsolateTables.strain">
        <columnAttribute name="strain" internal="true"/>
     </table>

     <table name="host" includeProjects="CryptoDB,PlasmoDB,ToxoDB,EuPathDB,GiardiaDB"
            displayName="Host"
            queryRef="IsolateTables.host">
