<wdkModel>
  <paramSet name="compoundParams" includeProjects="PlasmoDB,EuPathDB">
        
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- Compound IDs -->  
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

    <datasetParam name="ds_pubchem_compound_id"
		  recordClassRef="CompoundRecordClasses.CompoundRecordClass"
		  prompt="Compound ID">

      <help>
	Enter a PubChem compound ID.
      </help>
      <suggest
	  sample="3034391"
	  default="3034391"/>
    </datasetParam>

    <stringParam name="pubchem_compound_id" prompt="Compound ID" number="false">
      <help> Enter a PubChem compound ID.Wildcards (*) are allowed.</help>
      <suggest
	  sample="3034391"
	  default="3034391"/>
    </stringParam>

    <stringParam name="min_molecular_weight"
		 prompt="Min Molecular Weight (Daltons)"
		 number="true">
      <help>Lower bound for the molecular weight of compound</help>
      <suggest default="800"/>
    </stringParam>

    <stringParam name="max_molecular_weight"
		 prompt="Max Molecular Weight (Daltons)"
		 number="true">
      <help>Upper bound for the molecular weight of compound</help>
      <suggest default="900"/>
    </stringParam>

    <answerParam name="compound_result"
                  prompt="Input Result Set"
                  readonly="true"
                  visible="false">
       <recordClass ref="CompoundRecordClasses.CompoundRecordClass" />
    </answerParam>


    </paramSet>
</wdkModel>
