<wdkModel>
    
  <!-- ==================================================================== -->
  <!-- Params  -->
  <!-- ==================================================================== -->
  <paramSet name="SpanParams">


 <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- dataset param for dynspans by ID  -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

     <datasetParam name="sequenceId" recordClassRef="DynSpanRecordClasses.DynSpanRecordClass" 
             prompt="Dynamic Span ID">

         <help includeProjects="EuPathDB">
           Enter an identifier for a genomic sequence. The IDs correspond to the accession numbers in the GenBank records supplied by the sequencing centers. Wild cards are permitted in most queries, i.e. use AAEE* to ask for all &lt;i&gt; C.parvum &lt;/i&gt; contigs. Genes By Chromosomal Location does not allow wildcards.
	 </help>

         <help includeProjects="InitDB">
           FILL
	 </help>

	 <help includeProjects="TrichDB">
           Enter an identifier for a genomic sequence. The IDs correspond to the accession numbers in the GenBank records supplied by the sequencing centers. Wild cards are permitted in most queries, i.e. use DS11* to ask for all contigs that have identifiers that begin with DS11.
         </help>
         <help includeProjects="GiardiaDB">
           Enter an identifier for a genomic sequence. The IDs correspond to the accession numbers in the GenBank records supplied by the sequencing centers. Wild cards are permitted in most queries, i.e. use AACB0200* to ask for all contigs/scaffolds that have identifiers that begin with AACB0200. 
         </help>
         <help includeProjects="CryptoDB">
             Enter an identifier for a genomic contig. The IDs
             correspond to the accession numbers in the GenBank records
             supplied by the sequencing centers. Contig ID's from
             &lt;i&gt; C. parvum &lt;/i&gt; whole genome sequencing
             project begin with 'AAEE' and contig ID's for &lt;i&gt;
             C. hominis &lt;/i&gt; begin with 'AAEL'. The &lt;i&gt;
             C. parvum &lt;/i&gt; Chromosome 6 independently sequenced
             at MRC-LMB  before the whole genome project was completed
             is 'BX526834 '. Wild cards are permitted  in most queries,
             i.e. use AAEE* to ask for all &lt;i&gt; C. parvum
             &lt;/i&gt; contigs. 
         </help>
         <help includeProjects="PlasmoDB">
             Sequence IDs for &#60;i&#62; P. falciparum &#60;/i&#62; chromosomes are 'Pf3D7_01' through 'Pf3D7_14.'  Sequence IDs for &#60;i&#62; P. yoelii &#60;/i&#62; sequences begin with 'AABL'.  Sequence IDs for &#60;i&#62; P. vivax &#60;/i&#62; chromosomes begin with 'CM', while the contigs begin with 'AAK' or 'AY'. Sequence IDs of &#60;i&#62; P. chabaudi &#60;/i&#62; chromosomes start with 'chab', while the contigs begin with 'contig'. &#60;i&#62;P.knowlesi &#60;/i&#62; chromosome sequences start with 'PK' or 'chr'. &#60;i&#62;P. berghei &#60;/i&#62; sequences start with 'PB',  &#60;i&#62;P.reichenowi &#60;/i&#62; sequence start with 'Pr', while those of &#60;i&#62;P.gallinaceum &#60;/i&#62; start with 'Pg'. Wild cards are permitted  in most queries, i.e. use 'AABL*' to ask for all &#60;i&#62; P. yoelii &#60;/i&#62; sequences. Genes By Chromosomal Location does not allow wildcards.
         </help>
         <suggest includeProjects="EuPathDB"
             sample="AAEL010000057, AAEE01000009, BX526834, Pf3D7_04, AABL01000036, VIIa, TGG_994746"
             default="AAEL01000057,VIIb,Pf3D7_04,DS113177"/>
         <suggest includeProjects="InitDB"
             sample=""
             default="(Example: )"
             allowEmpty="true"
             emptyValue="No Match"/>
         <suggest includeProjects="AmoebaDB"
             sample="DS550549, DS571151, EINV_674"
             default="EINV_674"
             allowEmpty="true"
             emptyValue="No Match"/>
         <suggest includeProjects="MicrosporidiaDB"
             sample="Ein08, NC_003230"
             default="NC_003230"
             allowEmpty="true"
             emptyValue="No Match"/>
         <suggest includeProjects="CryptoDB"
             sample="AAEL01000057, AAEE01000009, BX526834"
             default="AAEE01000006"
             allowEmpty="true"
             emptyValue="No Match"/>
         <suggest includeProjects="ToxoDB"
             sample="Ia, VIIb, XI, VIIa, TGG_994746"
             default="(Examples: Ia, TGG_994746)"
             allowEmpty="true"
             emptyValue="No Match"/>
         <suggest includeProjects="PlasmoDB"
             sample="Pf3D7_04, AABL01000036, CM000452, PB_RP2830"
             default="(Examples: Pf3D7_04, PB_RP2830)"
             allowEmpty="true"
             emptyValue="No Match"/>     
         <suggest includeProjects="GiardiaDB"
             sample="AACB02000002, CH991779"
             default="CH991779"/>     
         <suggest includeProjects="TrichDB"
             sample="DS113177"
             default="DS113177"/>     
         <suggest includeProjects="TriTrypDB"
             sample="Lmjchr36"
             default="Lmjchr36"/>     
 
    </datasetParam>
 

        
        
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
    <!-- params for span logic operations -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

        
        <enumParam name="span_operation" prompt="Span Operation">
          <help>The Span Operation</help>
          <enumList>   
            <enumValue default="true">
              <term>overlap</term>
              <internal>overlap</internal>
            </enumValue>
            <enumValue>
              <term>a_contain_b</term>
              <internal>a_contain_b</internal>
            </enumValue>
            <enumValue>
              <term>b_contain_a</term>
              <internal>b_contain_a</internal>
            </enumValue>
           </enumList>   
        </enumParam>

        
        <enumParam name="span_strand" prompt="Select strand">
          <help>Select the strand(s) where thead feature is located.</help>
          <enumList>   
            <enumValue default="true">
              <term>Both strands</term>
              <internal>both_strands</internal>
            </enumValue>
            <enumValue>
              <term>Same strand</term>
              <internal>same_strand</internal>
            </enumValue>
            <enumValue>
              <term>Opposite strands</term>
              <internal>opposite_strands</internal>
            </enumValue>
          </enumList>   
        </enumParam>

        
        <enumParam name="span_output" prompt="Span Output Type">
          <help>The Span Operation</help>
          <enumList>   
            <enumValue default="true">
              <term>type of input A</term>
              <internal>a</internal>
            </enumValue>
            <enumValue>
              <term>type of input B</term>
              <internal>b</internal>
            </enumValue>
          </enumList>   
        </enumParam>

        <answerParam name="span_a" prompt="Input result A of Span Logic">
              <help>Select a result set as the input A of the Span Logic operation</help>
              <recordClass ref="GeneRecordClasses.GeneRecordClass" />
              <recordClass ref="OrfRecordClasses.OrfRecordClass" />
        </answerParam>

        
        <enumParam name="span_begin_a" prompt="begins at">
          <help>The anchor of the begin location</help>
          <enumList>   
            <enumValue default="true">
              <term>start</term>
              <internal>start</internal>
            </enumValue>
            <enumValue>
              <term>stop</term>
              <internal>stop</internal>
            </enumValue>
          </enumList>   
        </enumParam>
        
        <enumParam name="span_end_a" prompt="ends at">
          <help>The anchor of the end location</help>
          <enumList>   
            <enumValue>
              <term>start</term>
              <internal>start</internal>
            </enumValue>
            <enumValue default="true">
              <term>stop</term>
              <internal>stop</internal>
            </enumValue>
          </enumList>   
        </enumParam>

        
        <enumParam name="span_begin_direction_a" prompt="direction">
          <help>direction from the begin location</help>
          <enumList>   
            <enumValue default="true">
              <term>+</term>
              <internal>+</internal>
            </enumValue>
            <enumValue>
              <term>-</term>
              <internal>-</internal>
            </enumValue>
          </enumList>   
        </enumParam>
        
        <enumParam name="span_end_direction_a" prompt="direction">
          <help>direction from the end location</help>
          <enumList>   
            <enumValue default="true">
              <term>+</term>
              <internal>+</internal>
            </enumValue>
            <enumValue>
              <term>-</term>
              <internal>-</internal>
            </enumValue>
          </enumList>   
        </enumParam>


        <stringParam name="span_begin_offset_a"
                     prompt="begin offset" number="true">
            <help>The offset from the begin</help>
            <suggest default="0"/>
        </stringParam>

        <stringParam name="span_end_offset_a"
                     prompt="end offset" number="true">
            <help>The offset from the begin</help>
            <suggest default="0"/>
        </stringParam>
         
        
        
        <answerParam name="span_b" prompt="Input result B of Span Logic">
              <help>Select a result set as the input B of the Span Logic operation</help>
              <recordClass ref="GeneRecordClasses.GeneRecordClass" />
              <recordClass ref="OrfRecordClasses.OrfRecordClass" />
        </answerParam>
        
        <enumParam name="span_begin_b" prompt="begins at">
          <help>The anchor of the begin location</help>
          <enumList>   
            <enumValue default="true">
              <term>start</term>
              <internal>start</internal>
            </enumValue>
            <enumValue>
              <term>stop</term>
              <internal>stop</internal>
            </enumValue>
          </enumList>   
        </enumParam>
        
        <enumParam name="span_end_b" prompt="ends at">
          <help>The anchor of the end location</help>
          <enumList>   
            <enumValue>
              <term>start</term>
              <internal>start</internal>
            </enumValue>
            <enumValue default="true">
              <term>stop</term>
              <internal>stop</internal>
            </enumValue>
          </enumList>   
        </enumParam>

        
        <enumParam name="span_begin_direction_b" prompt="direction">
          <help>direction from the begin location</help>
          <enumList>   
            <enumValue default="true">
              <term>+</term>
              <internal>+</internal>
            </enumValue>
            <enumValue>
              <term>-</term>
              <internal>-</internal>
            </enumValue>
          </enumList>   
        </enumParam>
        
        <enumParam name="span_end_direction_b" prompt="direction">
          <help>direction from the end location</help>
          <enumList>   
            <enumValue default="true">
              <term>+</term>
              <internal>+</internal>
            </enumValue>
            <enumValue>
              <term>-</term>
              <internal>-</internal>
            </enumValue>
          </enumList>   
        </enumParam>


        <stringParam name="span_begin_offset_b"
                     prompt="begin offset" number="true">
            <help>The offset from the begin</help>
            <suggest default="0"/>
        </stringParam>

        <stringParam name="span_end_offset_b"
                     prompt="end offset" number="true">
            <help>The offset from the begin</help>
            <suggest default="0"/>
        </stringParam>

 
 </paramSet>

</wdkModel>
