       regexp_substr(oa.organism_name, '\w{1,}') as genus,
       regexp_substr(oa.organism_name, '\s\w{1,}') as species,
       oa.organism_name as organism,
       decode(oa.ncbi_tax_id, 0, null, oa.ncbi_tax_id) as ncbi_tax_id, 
       oa.genecount,
       oa.codinggenecount,
       oa.othergenecount,
       oa.pseudogenecount,
       oa.communitycount,
