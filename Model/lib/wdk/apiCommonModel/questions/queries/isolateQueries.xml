    <sqlQuery name="ByWeightFilter" isCacheable="true" doNotTest="true">
        <paramRef ref="isolateParams.isolate_result"/>
        <paramRef ref="sharedParams.min_weight" default="0"/>
        <paramRef ref="sharedParams.max_weight" default="100"/>
        <column name="project_id"/>
        <column name="source_id"/>
        <column name="wdk_weight" />
        <sql>
            <![CDATA[
            SELECT a.source_id, a.project_id, a.wdk_weight
            FROM $$isolate_result$$ a
            WHERE a.wdk_weight >= $$min_weight$$
              AND a.wdk_weight <= $$max_weight$$
            ]]>
       </sql>
    </sqlQuery>


     <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
     <!-- SNPs by Isolate transform -->
     <!--++++++++++++++++++++++++++++++++++++++++++++++++-->
 
     <sqlQuery name="SnpsByHTSIsolateId" includeProjects="PlasmoDB" isCacheable="true">

       <paramRef ref="isolateParams.htsIsolateList"/>
       <paramRef ref="organismParams.organism" displayType="listBox" multiPick="false" quote="true" queryRef="organismVQ.withHtsSNPs">
          <help>Select Organism you wish to query against.</help>
       </paramRef>
       <paramRef ref="sharedParams.htsSnp_strain_a"/>  
       <paramRef ref="sharedParams.hts_snps_coverage"/>
       <paramRef ref="sharedParams.hts_snps_allele_freq" default="80" includeProjects="PlasmoDB"/>
       <paramRef ref="sharedParams.hts_snps_pvalue"/>

       <column name="source_id" />
       <column name="project_id"/>
       <column name="gene"/>
       <column name="snp_location"/>
       <column name="ref_strain"/>
       <column name="comp_strains"/>
       <column name="ref_allele"/>
       <column name="comp_alleles"/>
       <column name="phenotype"/>
       <column name="prot_pos"/>
       <column name="ref_product"/>
       <column name="comp_products"/>
       <column name="wdk_weight" />

      <sql>
         <![CDATA[
WITH
  sa as 
    (select /*+ materialize */ source_id, gene_source_id, na_feature_id, start_min, position_in_protein, gene_strand
     from   APIDBTUNING.snpattributes
     where  dataset = 'InsertSnps.pm NGS SNPs INTERNAL' and type = 'HTS'),
  sv1 as
    (select /*+ materialize */ parent_id, organism, strain, allele, coverage, product 
     from  dots.seqvariation
     where organism = $$organism$$ 
       and strain = $$htsSnp_strain_a$$ 
       and (coverage >= $$hts_snps_coverage$$ or coverage is null) 
       and (allele_percent >= $$hts_snps_allele_freq$$ or allele_percent is null)
       and (pvalue <= $$hts_snps_pvalue$$ or pvalue is null)),
  sv2 as
    (select /*+ materialize */ parent_id, organism, strain, allele, coverage, product 
     from  dots.seqvariation
     where organism = $$organism$$ 
        and strain in (select a.strain  
                       from ApidbTuning.IsolateAttributes a, $$htsIsolateList$$ b 
                       where a.source_id=b.source_id and a.data_type='HTS') 
       and (coverage >= $$hts_snps_coverage$$ or coverage is null) 
       and (allele_percent >= $$hts_snps_allele_freq$$ or allele_percent is null)
       and (pvalue <= $$hts_snps_pvalue$$ or pvalue is null))
SELECT 
    snpq.source_id, '@PROJECT_ID@' as project_id, snpq.gene, snpq.snp_location, snpq.prot_pos,
    listagg(snpq.ref_allele,', ')  within group (order by snpq.ref_allele) as ref_allele,
    listagg(snpq.ref_product,', ')  within group (order by snpq.ref_product) as ref_product,
    listagg(snpq.comp_allele,', ')  within group (order by snpq.comp_allele) as comp_alleles,
    listagg(snpq.comp_product,', ')  within group (order by snpq.comp_product) as comp_products,
    listagg(snpq.phenotype,', ')  within group (order by snpq.phenotype) as phenotype,
    listagg(snpq.ref_strain,', ')  within group (order by snpq.ref_strain) as ref_strain,
    listagg(snpq.comp_strain,', ')  within group (order by snpq.comp_strain) as comp_strains,
    0 as wdk_weight
FROM
(
SELECT sa.source_id,
      sa.gene_source_id as gene,
      sa.start_min as snp_location,
      sa.position_in_protein as prot_pos,
      --CASE WHEN sa.gene_strand = 'reverse' THEN apidb.reverse_complement(sv1.allele)
      --     ELSE sv1.allele 
      --     END as ref_allele, 
      sv1.allele as ref_allele, 
      sv1.product as ref_product,
      --CASE WHEN sa.gene_strand = 'reverse' THEN apidb.reverse_complement(sv2.allele)
      --    ELSE sv2.allele 
      --     END as comp_allele, 
      sv2.allele as comp_allele, 
      sv2.product as comp_product,
      CASE WHEN sv1.product = sv2.product then 'syn' 
           WHEN sv1.product != sv2.product THEN 'non-syn'
           ELSE 'non-coding' 
      END as phenotype,
      sv1.strain as ref_strain, 
      sv2.strain as comp_strain
FROM  sa, sv1, sv2
WHERE sa.na_feature_id = sv1.parent_id
  and sa.na_feature_id = sv2.parent_id
  and sv1.allele != sv2.allele
) snpq
GROUP BY snpq.source_id, snpq.gene, snpq.snp_location, snpq.prot_pos
            ]]>
        </sql>
     </sqlQuery>

    </querySet>

