            <questionRef>GeneQuestions.GenesWithUserComments</questionRef>
	    <questionRef includeProjects="TriTrypDB,PlasmoDB,EuPathDB">GeneQuestions.GenesWithUpdatedAnnotation</questionRef>
            <questionRef>GeneQuestions.GenesByMr4Reagents</questionRef> 
	    <questionRef includeProjects="GiardiaDB">GeneQuestions.GenesByGeneDeprecationStatus</questionRef> 
