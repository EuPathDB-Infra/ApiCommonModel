            <![CDATA[
                     SELECT talon_transcript_name as long_read_transcript_id
                          , jsonkey as sample
                          , jsonvalue as count
                     FROM apidb.longreadtranscript,apidb.get_json_vals(count_data)
            ]]>
            </sql>
        </sqlQuery>

    </querySet>
</wdkModel>
