<wdkModel>
    <querySet name="LongReadTranscriptTables" queryType="table" isCacheable="false" excludeProjects="EuPathDB,VEuPathDB">
        <sqlQuery name="SampleInfo"  isCacheable='false'>
            <column name="long_read_transcript_id" />
            <column name="sample" />
            <column name="count" />
