        <sqlQuery name="LongReadTranscriptAlias" doNotTest="true">
            <column name="long_read_transcript_id"/>
            <column name="old_long_read_transcript_id"/>
            <sql>
              <![CDATA[
              SELECT talon_transcript_name as long_read_transcript_id
                   , talon_transcript_name as old_long_read_transcript_id
                FROM Apidb.longreadtranscript
              ]]>
            </sql>
        </sqlQuery>

       <sqlQuery name="Bfmv">
            <column name="na_seq_source_id"/>
            <column name="min_start"/>
            <column name="max_end"/>
            <column name="gene_source_id"/>
            <column name="transcript_source_id"/>
            <column name="long_read_transcript_id"/>
            <column name="number_of_exon"/>
            <column name="transcript_length"/>
            <column name="gene_novelty"/>
            <column name="transcript_novelty"/>
            <column name="incomplete_splice_match_type"/>

            <sql>
            <![CDATA[
              SELECT gene_source_id
                   , transcript_source_id
                   , talon_transcript_name as long_read_transcript_id
                   , number_of_exon
                   , transcript_length
                   , gene_novelty
                   , transcript_novelty
                   , incomplete_splice_match_type
                   , min_start
                   , max_end
                   , na_seq_source_id
                FROM Apidb.longreadtranscript
            ]]>
           </sql>
        </sqlQuery>

    </querySet>


</wdkModel>
