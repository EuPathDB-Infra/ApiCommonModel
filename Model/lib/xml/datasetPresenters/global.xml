      <url><![CDATA[http://www.expasy.org/enzyme]]></url>
    </link>
    <pubmedId>10592255</pubmedId>
    <templateInjector className="org.apidb.apicommon.model.datasetInjector.EnzymeNumbers"/>


  </datasetPresenter>





