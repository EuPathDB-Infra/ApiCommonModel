 <!ELEMENT dataSourceAttributions (dataSourceAttribution+,internalDataSources*)>
 <!ELEMENT dataSourceAttribution (displayCategory?,publications?,contacts,displayName,summary,protocol?,caveat?,acknowledgement?,releasePolicy?,description,links?,wdkReference*)>
 <!ELEMENT displayCategory (#PCDATA)>
 <!ELEMENT publications (publication+)>
 <!ELEMENT publication (#PCDATA)>
 <!ELEMENT contacts (contact+)>
 <!ELEMENT contact (name,email?,institution,address?,city?,state?,country?,zip?)>
 <!ELEMENT name (#PCDATA)>
 <!ELEMENT email (#PCDATA)>
 <!ELEMENT institution (#PCDATA)>
 <!ELEMENT address (#PCDATA)>
 <!ELEMENT city (#PCDATA)>
 <!ELEMENT state (#PCDATA)>
 <!ELEMENT country (#PCDATA)>
 <!ELEMENT zip (#PCDATA)>
 <!ELEMENT displayName (#PCDATA)>
 <!ELEMENT summary (#PCDATA)>
 <!ELEMENT protocol (#PCDATA)>
 <!ELEMENT caveat (#PCDATA)>
 <!ELEMENT acknowledgement (#PCDATA)>
 <!ELEMENT releasePolicy (#PCDATA)>
 <!ELEMENT description (#PCDATA)>
 <!ELEMENT links (link+)>
 <!ELEMENT link (type,url,linkDescription?)>
 <!ELEMENT type (#PCDATA)>
 <!ELEMENT url (#PCDATA)>
 <!ELEMENT linkDescription (#PCDATA)>
 <!ELEMENT wdkReference (text*)>
 <!ELEMENT text (#PCDATA)>
 <!ELEMENT internalDataSources (resource*)>
 <!ELEMENT resource (#PCDATA)>
  
<!ATTLIST dataSourceAttribution resource CDATA #REQUIRED>
<!ATTLIST dataSourceAttribution overridingType CDATA #IMPLIED>
<!ATTLIST dataSourceAttribution overridingSubtype CDATA #IMPLIED>
<!ATTLIST dataSourceAttribution ignore CDATA #IMPLIED>
<!ATTLIST dataSourceAttribution nameIsRegEx CDATA #IMPLIED>
<!ATTLIST publication pmid CDATA #IMPLIED>
<!ATTLIST contact isPrimaryContact CDATA #IMPLIED>
<!ATTLIST wdkReference
          recordClass CDATA #REQUIRED
          type CDATA #REQUIRED
          name CDATA #REQUIRED
>
<!ATTLIST text name CDATA #REQUIRED>
<!ATTLIST resource nameIsRegEx CDATA #IMPLIED>
