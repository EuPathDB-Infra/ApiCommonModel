<?xml version="1.0" ?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="datasetPresenters">
      <oneOrMore>
        <element name="datasetPresenter">
          <attribute name="datasetDisplayName" />
          <attribute name="datasetShortDisplayName" />
          <attribute name="organismShortName" />
          <attribute name="projectName" />
          <attribute name="buildNumberIntroduced" />
          <element name="description">
            <text/>
          </element>
          <zeroOrMore>
	    <element name="templateInjector">
	      <attribute name="class"/>
	      <zeroOrMore>
		<element name="prop">
		  <attribute name="name"/>
		  <text/>
		</element>
	      </zeroOrMore>
	    </element>
          </zeroOrMore>
        </element>
      </oneOrMore>
    </element>
  </start>
</grammar>
